(lex.add.entry '("nos_ADJ:pos" "ADJ:pos" ((("n" "o") 0))))
(lex.add.entry '("liens_NOM" NOM ((("l" "j" "ehn") 0))))
(lex.add.entry '("historiques_ADJ" ADJ ((("i" "s") 0) (("t" "oh") 0) (("rh" "i")0) (("k" ae) 0))))
(lex.add.entry '("sont_VER" VER ((("s" "ohn") 0))))
(lex.add.entry '("évidents_ADJ" ADJ ((("e") 0) (("v" "i") 0) (("d" "ahn") 0))))

(set! list_exclus*-> (append (list "liens") list_exclus*->_default))
(set! never_exception_list (append (list "sont" ) never_exception_list_default))
(dSayText "Nos liens historiques sont évidents.") ; neut_parl_s04_0631
